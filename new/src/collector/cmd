CREATE TABLE IF NOT EXISTS `amount_history`(
   `tag` varchar(255),
   `amount` DECIMAL(10, 2),
   `timestamp` varchar(255) NOT NULL,
   PRIMARY KEY ( `timestamp`,`tag` )
)ENGINE=InnoDB DEFAULT CHARSET=utf8;